    <scm>
        <tag>${version}</tag>
        <url>https://github.com/pengweizhong/dynamic-sql2</url>
        <connection>https://github.com/pengweizhong/dynamic-sql2.git</connection>
    </scm>

    <dependencies>
        <!-- https://mvnrepository.com/artifact/org.junit.jupiter/junit-jupiter-api -->
